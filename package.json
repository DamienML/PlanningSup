{
  "name": "planningsup",
  "version": "2.5.4",
  "private": true,
  "scripts": {
    "build": "node --max-old-space-size=2048 node_modules/nuxt/bin/nuxt.js build",
    "dev": "docker-compose -f docker-compose-dev.yml up -d --remove-orphans && NO_BREE=true node --inspect --max-old-space-size=2048 node_modules/nuxt/bin/nuxt.js",
    "dev-server": "docker-compose -f docker-compose-dev.yml up -d --remove-orphans && NO_BREE=true nodemon --inspect --max-old-space-size=2048 server/",
    "dev-server:bree": "docker-compose -f docker-compose-dev.yml up -d --remove-orphans && nodemon --inspect --max-old-space-size=2048 server/",
    "dev:bree": "docker-compose -f docker-compose-dev.yml up -d --remove-orphans && node --inspect --max-old-space-size=2048 node_modules/nuxt/bin/nuxt.js",
    "generate": "nuxt generate",
    "preinstall": "npx only-allow pnpm",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "start": "node --max-old-space-size=2048 node_modules/nuxt/bin/nuxt.js start",
    "test": "docker-compose -f docker-compose-dev.yml up -d --remove-orphans && NODE_ENV=test mocha --exit test/calendar.test.js test/urls.test.js"
  },
  "dependencies": {
    "@dansmaculotte/nuxt-security": "^0.0.5",
    "@ladjs/graceful": "^1.1.5",
    "@mdi/js": "^6.5.95",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/component-cache": "^1.1.6",
    "@nuxtjs/google-fonts": "^1.3.0",
    "@nuxtjs/moment": "^1.6.1",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/vuetify": "^1.12.3",
    "axios": "^0.26.1",
    "bip39": "^3.0.4",
    "body-parser": "^1.19.2",
    "bree": "^7.2.0",
    "cal-parser": "^1.0.2",
    "config": "^3.3.7",
    "connect-mongodb-session": "^3.1.1",
    "cookie-parser": "^1.4.6",
    "cookie-universal-nuxt": "^2.1.5",
    "core-js": "^3.21.1",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "express-session": "^1.17.2",
    "jsdom": "^19.0.0",
    "luxon": "^2.3.1",
    "minify-css-string": "^1.0.0",
    "mongoose": "^6.2.7",
    "ms": "^2.1.3",
    "node-libcurl": "^2.3.4",
    "nuxt": "^2.15.8",
    "nuxt-json-config": "^0.0.2",
    "nuxt-start": "^2.15.8",
    "route-cache": "^0.4.7",
    "sanitize-html": "^2.7.0",
    "signale": "^1.4.0",
    "v-tooltip": "^2.1.3",
    "vue-plausible": "^1.3.1",
    "vue-swatches": "^2.1.1",
    "vuetify": "^2.6.4",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/eslint-parser": "^7.17.0",
    "@fullhuman/postcss-purgecss": "^3.1.3",
    "@nuxtjs/eslint-config": "^8.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "chai": "^4.3.6",
    "css-byebye": "^3.1.0",
    "eslint": "^8.11.0",
    "eslint-plugin-nuxt": "^3.2.0",
    "mocha": "^9.2.2",
    "nodemon": "^2.0.15",
    "sass-loader": "10",
    "supertest": "^6.2.2"
  },
  "engines": {
    "node": "v16"
  }
}
